gcloud beta notebooks instances create my-private-ai-workbench \
    --location=us-central1 \
    --machine-type=n1-standard-4 \
    --subnet=projects/my-project/regions/us-central1/subnetworks/my-private-subnet \
    --no-public-ip \
    --private-networking \
    --service-account=my-service-account@my-project.iam.gserviceaccount.com \
    --metadata=enable-oslogin=true \
    --metadata=notebooks-api=true \
    --metadata=proxy-mode=service_account \
    --metadata=single-user-email=john.doe@example.com \
    --metadata=allowed-user-accounts=ad-group@example.com \
    --shielded-secure-boot \
    --no-enable-ip-forwarding \
    --kms-key=projects/my-project/locations/us-central1/keyRings/my-keyring/cryptoKeys/my-cmek-key \
    --maintenance-window-day=saturday \
    --maintenance-window-hour=23 \
    --maintenance-window-tz=America/New_York
